{{ page.header }}
<section class="innerbanner">
    {{ page.banner }}
    <div class="container">
    </div>
</section>

<section id="job-details" class="mainWrapper">
    <div class="container">
        {{ page.breadcrumb }}
        {{ page.help }}
    </div>
    {% if logged_in %}
        <div class="container">
            <div class="left-col">
                {{ page.content }}
            </div>
            <div class="right-col">
                {% if page.sidebar_first|render %}
                    {{ page.sidebar_first }}
                {% endif %}
            </div>
        </div>
        {{ page.content_middle_one }}
      
        {{ page.content_middle_two }}
        {{ page.content_bottom }}
        {{ title_prefix }}
    {% else %}
        <div class="detailContent trigger">
            <div class="managementLest">
                <h2>You need to log in to view this page</h2>
            </div>
        </div>
    {% endif %}
</section><!-- /#content -->

      {% if page.sidebar_second|render %}
        <aside id="sidebar-second" class="column sidebar second">
          {{ page.sidebar_second }}
        </aside><!-- /#sidebar-second -->
      {% endif %}
  <!-- ______________________ FOOTER _______________________ -->
{% if page.footer|render %}
  <footer><div class="container footer-container">{{ page.footer }}</div></footer>
{% endif %}


